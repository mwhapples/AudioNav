<?xml version="1.0" encoding="utf-8" ?>
<rxmaui:ReactiveContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:rxmaui="clr-namespace:ReactiveUI.Maui;assembly=ReactiveUI.Maui"
             xmlns:vm="clr-namespace:AudioNav.ViewModels"
                            x:TypeArguments="vm:FilterRateViewModel"
             x:Class="AudioNav.Views.FilterRateView">
    <VerticalStackLayout>
        <Label
                Text="Filter rate"
                FontSize="Header"
                SemanticProperties.HeadingLevel="Level2"
/>
        <Slider 
                x:Name="CompassFilterRateSlider"
                Minimum="0"
                Maximum="20"/>
    </VerticalStackLayout>
</rxmaui:ReactiveContentView>
